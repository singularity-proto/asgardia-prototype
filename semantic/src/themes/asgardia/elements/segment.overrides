/*******************************
         Theme Overrides
*******************************/

/* edges */
.ui.edged.segment,
.ui.edged.segment .ui.segment {
  border-radius: 0 !important;
}

/* colors */
.ui.red.segment:not(.inverted) {
  border-color: rgb(219,40,40);
  background-color: rgb(219,40,40,.8);
}

.ui.red.light.segment:not(.inverted) {
  background-color: rgba(219,40,40,.2);
}

.ui.red.colored.segment:not(.inverted) {
  border-top: 1px solid rgb(219,40,40) !important;
}

.ui.orange.segment:not(.inverted) {
