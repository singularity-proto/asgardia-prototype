/* docking */

.ui.docked.top.segments,
.ui.docked.top.segment {
  border-top-width: 0;
  border-top-left-radius: 0px;
  border-top-right-radius: 0px;
}

.ui.docked.bottom.segments,
.ui.docked.bottom.segment {
  border-bottom-width: 0;
  border-bottom-left-radius: 0px;
  border-bottom-right-radius: 0px;
}

.ui.docked.left.segments,
.ui.docked.left.segment {
  border-left-width: 0;
  border-top-left-radius: 0px;
  border-bottom-left-radius: 0px;
}

.ui.docked.right.segments,
.ui.docked.right.segment {
  border-right-width: 0;
  border-top-right-radius: 0px;
  border-bottom-right-radius: 0px;
}

.ui.segments .ui.segment:first-child {
  border-top-width: 0 !important;
}

.ui.segments .ui.segment:not(:first-child) {
  border-color: #424242 !important;
}

/* segment stack border colors */

.ui.red.segments, .ui.red.segments > .ui.segment {
