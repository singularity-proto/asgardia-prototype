/* alternate fonts */

@import url('https://fonts.googleapis.com/css?family=Exo+2');

/* docking */

.ui.docked.top {
  margin-top: -1em !important;
}

.ui.docked.bottom {
  margin-bottom: -1em !important;
}

.ui.docked.left {
